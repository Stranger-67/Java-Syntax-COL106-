package okay;
public class COL106 {
	public int age ;
	public static int credits ;
	public void intro(int a)
	{
		age = a ;
	}
	public void setage ( int newage )
	{
		age = newage ;
	}
	public static int getcredit ()
	{
		return credits ;
	}
	public static void main(String[] args) {
		//to use non-static methods and variables
		COL106 abc= new COL106();
		abc.intro(20);
		System.out.println(abc.age);
		abc.setage(69);
		System.out.println(abc.age);
		//To use static methods and variables
		credits= 22;
		System.out.println(getcredit());
	}
}

//output is:-
20
69
22
